{
  "name": "Minecraft Online Users",
  "version": "1.0",
  "manifest_version": 2,
  "description": "Extension which shows online users on minecraft servers",
  "browser_action": {
    "default_icon": "favicon.ico",
    "default_popup": "popup.html"
  },
  "permissions": [
    "notifications",
    "<all_urls>"
  ],
  "background": {
    "page": "background.html"
  },
  "options_page": "options.html"

}